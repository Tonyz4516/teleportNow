{
	"manifest_version": 3,
	"name": "Teleport Now",
	"version": "1.0.0",
	"description": "Sample description",
	"short_name": "teleport_now",
	"permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["background.css"],
			"js": ["contentscript.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icons/icon16.png"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"options_page":"index.html"
}